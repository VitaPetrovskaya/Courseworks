        <TextBlock x:Name="lableDi" Text="Ужин" HorizontalAlignment="Center" Margin="605,114,119.333,0" VerticalAlignment="Top" FontFamily="Bodoni Bd BT" Width="169" Foreground="#FF5284C0" RenderTransformOrigin="0.969,0.625" Height="147" FontSize="14" MouseDown="lableDi_MouseDown"/>

    </Grid>
