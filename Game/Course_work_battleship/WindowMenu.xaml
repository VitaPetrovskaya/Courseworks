        <TextBlock x:Name="lableMenu" Text="Рациональное меню для победителя!" HorizontalAlignment="Center" Margin="159,10,127.333,0" VerticalAlignment="Top" FontFamily="Bodoni Bd BT" Width="407" Foreground="#FF5284C0" RenderTransformOrigin="0.969,0.625" Height="33" FontSize="22" Background="{x:Null}"/>
