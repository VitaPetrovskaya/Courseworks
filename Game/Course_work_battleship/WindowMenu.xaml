<Window x:Class="Course_work_game.WindowMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Course_work_game"
        mc:Ignorable="d"
        Title="WindowMenu" Height="300" Width="900" Loaded="Window_Loaded">
    <Grid>
        <TextBlock x:Name="lableBreakfast" Text="Завтрак" TextWrapping="Wrap" HorizontalAlignment="Center" Margin="10,62,660.333,0" VerticalAlignment="Top" FontFamily="Bodoni Bd BT" Width="223" Foreground="#FF28A64E" RenderTransformOrigin="0.969,0.625" Height="47" FontSize="14"/>
        <TextBlock x:Name="lableLunch" Text="Обед" HorizontalAlignment="Center" Margin="283,62,293.333,0" VerticalAlignment="Top" FontFamily="Bodoni Bd BT" Width="317" Foreground="#FF8C4323" RenderTransformOrigin="0.969,0.625" Height="47" FontSize="14"/>
