            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="0.156"/>
                    <RotateTransform/>
                    <TranslateTransform X="0.33"/>
                </TransformGroup>
            </TextBox.RenderTransform>
